<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <include file="example.xml"/>
    <!-- Configuration for the default package. -->
    <package name="default" extends="struts-default" namespace="/">
        <action name="InsertarLibroInit" class="com.lenguajes.a80703.application.action.InsertarLibroAction">
            <result name="input" type="dispatcher">insertarLibro.jsp</result>
        </action>
        <action name="InsertarLibro" class="com.lenguajes.a80703.application.action.InsertarLibroAction">
            <result name="input" type="dispatcher">insertarLibro.jsp</result>
        </action>
        <action name="InsertarLibroSalvar" method="salvar" class="com.lenguajes.a80703.application.action.InsertarLibroAction">
            <result name="success" type="dispatcher">insertarLibro.jsp</result>
        </action>
        <action name="InsertarLibroExcluir" method="incluirAutor" class="com.lenguajes.a80703.application.action.InsertarLibroAction">
            <result name="input" type="dispatcher">insertarLibro.jsp</result>
        </action>
        <action name="InsertarLibroIncluir" method="exlcuirAutor" class="com.lenguajes.a80703.application.action.InsertarLibroAction">
            <result name="input" type="dispatcher">insertarLibro.jsp</result>
        </action>        
        
    </package>
</struts>
